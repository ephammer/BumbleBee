<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bumblebeem.android.bumblebeem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/honeycomb_app_icon"
        android:supportsRtl="true"
        android:theme="@style/BeeTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivity"
            android:label="Game">

            <!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>
        <activity
            android:name=".ResultsActivity"
            android:label="Results" />
        <activity android:name=".GameLevelActivity" />
        <activity android:name=".GameChargerActivity" />
        <activity android:name=".GameFileActivity"></activity>
    </application>

</manifest>